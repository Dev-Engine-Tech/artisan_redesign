<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
<dict>
	<key>CFBundleDevelopmentRegion</key>
	<string>$(DEVELOPMENT_LANGUAGE)</string>
	<key>CFBundleDisplayName</key>
	<string>Artisans Circle</string>
	<key>CFBundleExecutable</key>
	<string>$(EXECUTABLE_NAME)</string>
	<key>CFBundleIdentifier</key>
	<string>$(PRODUCT_BUNDLE_IDENTIFIER)</string>
	<key>CFBundleInfoDictionaryVersion</key>
	<string>6.0</string>
	<key>CFBundleName</key>
	<string>Artisans Circle</string>
	<key>CFBundlePackageType</key>
	<string>APPL</string>
	<key>CFBundleShortVersionString</key>
	<string>$(FLUTTER_BUILD_NAME)</string>
	<!-- Google Sign-In iOS client id for google_sign_in plugin -->
	<key>GIDClientID</key>
	<string>1025352562164-691cmsv71snn3s9igp09tat8l6fq1nsp.apps.googleusercontent.com</string>
	<key>CFBundleSignature</key>
	<string>????</string>
	<key>CFBundleVersion</key>
	<string>$(FLUTTER_BUILD_NUMBER)</string>
	<key>LSRequiresIPhoneOS</key>
	<true/>
	<key>UILaunchStoryboardName</key>
	<string>LaunchScreen</string>
	<key>UIMainStoryboardFile</key>
	<string>Main</string>
	<key>UISupportedInterfaceOrientations</key>
	<array>
		<string>UIInterfaceOrientationPortrait</string>
		<string>UIInterfaceOrientationLandscapeLeft</string>
		<string>UIInterfaceOrientationLandscapeRight</string>
	</array>
	<key>UISupportedInterfaceOrientations~ipad</key>
	<array>
		<string>UIInterfaceOrientationPortrait</string>
		<string>UIInterfaceOrientationPortraitUpsideDown</string>
		<string>UIInterfaceOrientationLandscapeLeft</string>
		<string>UIInterfaceOrientationLandscapeRight</string>
	</array>
	<key>CADisableMinimumFrameDurationOnPhone</key>
	<true/>
	<key>UIApplicationSupportsIndirectInputEvents</key>
	<true/>
	<key>NSCameraUsageDescription</key>
	<string>This app requires access to the camera to capture video and images for job submissions and chat media sharing.</string>
    <key>NSPhotoLibraryUsageDescription</key>
    <string>This app requires access to your photo library to allow users to upload images for their profile and share media within the app.</string>
    <key>NSPhotoLibraryAddUsageDescription</key>
    <string>This app may save images to your photo library when needed.</string>
    <key>NSMicrophoneUsageDescription</key>
    <string>This app requires access to the microphone to record audio.</string>
    <key>NSDocumentsFolderUsageDescription</key>
    <string>This app needs access to save the recorded audio files.</string>
    <key>NSLocationAlwaysAndWhenInUseUsageDescription</key>
    <string>This app requires access to your location to provide personalized content and improve user experience based on your geographical location.</string>
    <key>NSLocationWhenInUseUsageDescription</key>
    <string>This app requires access to your location when in use to display relevant content based on your current location.</string>
    <key>NSRemoteNotificationPermitted</key>
    <true/>
    <key>ITSAppUsesNonExemptEncryption</key>
    <false/>

	<!-- URL schemes required for Google Sign-In callback. Replace the placeholder with your actual reversed client ID
	 obtained from GoogleService-Info.plist (REVERSED_CLIENT_ID). Example: com.googleusercontent.apps.1234567890-abcdefg -->
	<key>CFBundleURLTypes</key>
	<array>
		<dict>
			<key>CFBundleTypeRole</key>
			<string>Editor</string>
			<key>CFBundleURLSchemes</key>
			<array>
				<string>com.googleusercontent.apps.1025352562164-691cmsv71snn3s9igp09tat8l6fq1nsp</string>
			</array>
		</dict>
	</array>

	<!-- Allow Google sign-in to query installed apps -->
	<key>LSApplicationQueriesSchemes</key>
	<array>
		<string>google</string>
		<string>com.googleusercontent.apps.1025352562164-691cmsv71snn3s9igp09tat8l6fq1nsp</string>
	</array>

	<!-- Enable background modes for remote notifications -->
	<key>UIBackgroundModes</key>
	<array>
		<string>remote-notification</string>
		<string>fetch</string>
	</array>

	<!-- Allow WebView to load content from external sources including YouTube -->
	<key>NSAppTransportSecurity</key>
	<dict>
		<key>NSAllowsArbitraryLoads</key>
		<true/>
	</dict>

	<!-- Allow inline media playback in WebView -->
	<key>io.flutter.embedded_views_preview</key>
	<true/>
</dict>
</plist>
